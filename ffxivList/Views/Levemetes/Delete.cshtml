@model ffxivList.Models.Levemete

@{
    ViewData["Title"] = "Delete Levemete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this levemete?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <input type="hidden" asp-for="LevemeteID" />
        <dt>
           Name
        </dt>
        <dd>
            @Html.DisplayFor(model => model.LevemeteName)
        </dd>
        <dt>
            Level
        </dt>
        <dd>
            @Html.DisplayFor(model => model.LevemeteLevel)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            <a asp-action="IndexAdmin">Back to List</a>
        </div>
    </form>
</div>
