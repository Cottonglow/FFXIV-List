@model List<ffxivList.Models.Levemete>

@{
    ViewData["Title"] = "Levemetes";
}
<br />
<h2>@ViewData["Title"]</h2>
<br />
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                Level
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.LevemeteName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LevemeteLevel)
                </td>
            </tr>}
    </tbody>
</table>
